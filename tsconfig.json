{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017",
    ],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": "./src",
    "noImplicitAny": false,
  }
}